}

.message .nav >li.open>a {
    /* background-color: #2dc3e8; */
    -webkit-box-shadow: 0 2px 40px rgba(0,0,0,.4);
    -moz-box-shadow: 0 2px 40px rgba(0,0,0,.4);
    box-shadow: 0 2px 40px rgba(0,0,0,.4);
}

.message .nav >li>a .icon {
    display: block;
