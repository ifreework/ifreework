/*********************************页面顶部******************************************/
.navbar{
	background:rgba(0,0,0,.1);
	padding: 5px  10px;
	margin: 0;
}
.navbar .logo{
	text-shadow: 5px -5px #5aaf54, 4px -4px white;
	font-weight: bold;
	font-size:30px;
	-webkit-text-fill-color: transparent;
	-webkit-background-clip: text ;
}
.navbar .logo,.navbar .content,.navbar .logo .logo_img,.navbar .logo .logo_text{
	display: inline-block;
	float: left;
}
.navbar .logo .logo_text h1{
	margin:18px 0 0 0px;
	font-size:28px;
	font-weight: bold;
}
.navbar .content{
	padding: 10px 10px 0px 15px;
}

.navbar .content .content-body{
	display: block;
	padding:5px 20px;
	height: 30px;
	width:1000px;
	line-height:18px;
	border: 1px solid rgba(0, 0, 0, 0.2);
    border-radius: 30px;
	background: rgba(8,8,8,.2);
}
.content-body #date,.content-body #time,.content-body #weather{
	display: inline-block;
	color: #fff;
	
	margin-left: 5px;
